{
  "name": "Mini Fiddler",
  "description": "观察、修改网络请求",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["activeTab", "webRequest", "webRequestBlocking", "https://*/*", "http://*/*"],
  "devtools_page": "devtools.html",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png"
    },
    "default_title": "Mini Fiddler"
  },
  "icons": {
    "128": "icons/fiddler.png"
  },
  "externally_connectable": {
    "ids": []
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
